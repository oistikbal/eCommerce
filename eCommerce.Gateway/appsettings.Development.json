{
    "Logging": {
        "LogLevel": {
            "Default": "Trace",
            "Microsoft.AspNetCore": "Trace",
            "Yarp.ReverseProxy": "Trace"
        }
    },
    "Kestrel": {
        "Endpoints": {
            "Grpc": {
                "Url": "https://localhost:8080",
                "Protocols": "Http2"
            }
        }
    },
    "ReverseProxy": {
        "Routes": {
            "user-route-v1": {
                "RouteId": "user-route-v1-id",
                "ClusterId": "user-cluster",
                "Match": {
                    "Path": "/eCommerce.UserService.v1.UserService/{**catch-all}"
                }
            },
            "auth-route-v1": {
                "RouteId": "auth-route-v1-id",
                "ClusterId": "user-cluster",
                "Match": {
                    "Path": "/eCommerce.UserService.v1.AuthService/{**catch-all}"
                },
                "AuthorizationPolicy": "Administrator"
            }
        },
        "Clusters": {
            "user-cluster": {
                "Destinations": {
                    "user": {
                        "Address": "https://localhost:5000"
                    }
                }
            }
        }
    }
}