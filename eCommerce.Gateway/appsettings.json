{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ReverseProxy": {
        "Routes": {
            "user-route": {
                "RouteId": "user-route-id",
                "ClusterId": "user-cluster",
                "Match": {
                    "Path": "userservice.UserService/{**catch-all}"
                },
                "Transforms": [
                    { "RequestHeadersCopy": "true" }
                ]
            }
        },
        "Clusters": {
            "user-cluster": {
                "Destinations": {
                    "user": {
                        "Address": "https://localhost:5001"
                    }
                }
            }
        }
    }
}
