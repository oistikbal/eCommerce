{
    "Logging": {
        "LogLevel": {
            "Default": "Information"
        }
    },
    "AllowedHosts": "localhost",
    "Kestrel": {
        "Endpoints": {
            "Grpc": {
                "Url": "https://localhost:8080",
                "Protocols": "Http2"
            }
        }
    },
    "JwtSettings": {
        "SecretKey": "aB9zV0NkT5xZ7k/JGQhP5w==D12312DASD",
        "Issuer": "eCommerce",
        "Audience": "eCommerceAPI"
    },
    "ReverseProxy": {
        "Routes": {
            "user-route-v1": {
                "RouteId": "user-route-v1-id",
                "ClusterId": "user-cluster",
                "Match": {
                    "Path": "/eCommerce.UserService.v1.UserService/{**catch-all}"
                }
            },
            "auth-route-v1": {
                "RouteId": "auth-route-v1-id",
                "ClusterId": "user-cluster",
                "Match": {
                    "Path": "/eCommerce.UserService.v1.AuthService/{**catch-all}"
                }
            },
            "auth-change-password-route-v1": {
                "RouteId": "change-password-route-id",
                "ClusterId": "user-cluster",
                "Match": {
                    "Path": "/eCommerce.UserService.v1.AuthService/ChangePassword"
                },
                "AuthorizationPolicy": "Default"
            },
            "auth-change-email-route-v1": {
                "RouteId": "change-email-route-id",
                "ClusterId": "user-cluster",
                "Match": {
                    "Path": "/eCommerce.UserService.v1.AuthService/ChangeEmail"
                },
                "AuthorizationPolicy": "Default"
            },
            "auth-check-user-role-route-v1": {
                "RouteId": "check-user-role-route-id",
                "ClusterId": "user-cluster",
                "Match": {
                    "Path": "/eCommerce.UserService.v1.AuthService/CheckUserRole"
                },
                "AuthorizationPolicy": "Administrator"
            }
        },
        "Clusters": {
            "user-cluster": {
                "Destinations": {
                    "user": {
                        "Address": "https://localhost:5000"
                    }
                }
            }
        }
    }
}