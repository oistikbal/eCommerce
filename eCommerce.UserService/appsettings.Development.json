{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ReverseProxy": {
        "Routes": {
            "userRoute": {
                "ClusterId": "userCluster",
                "Match": {
                    "Path": "/user/{**catch-all}"
                }
            }
        },
        "Clusters": {
            "userCluster": {
                "Destinations": {
                    "userService": {
                        "Address": "http://localhost:5001"
                    }
                }
            }
        }
    },
    "ConnectionStrings": {
        "DefaultConnection": "Data Source=UserService.sqlite"
    }
}
